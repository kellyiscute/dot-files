{
	"npm.binPath": "pnpm",
	"coc.preferences.formatOnSaveFiletypes": ["ts", "tsx", "jsx", "js", "css", "scss", "less", "go"],
	"coc.preferences.formatOnTypeFiletypes": ["ts", "tsx", "jsx", "js", "css", "scss", "less", "go"],
  "coc.preferences.enableMessageDialog": true,
	"prettier.printWidth": 100,
	"suggest.noselect": false,
	"explorer.icon.enableNerdfont": true,
	"explorer.git.enable": true,
	"explorer.file.git.showUntrackedFiles": true,
	"explorer.mouseMode": "singleclick",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.preferences.quoteStyle": "double",
  "git.addGBlameToVirtualText": true,
  "prettier.enable": true,
  "typescript.format.enabled": false,
  "tsserver.useLocalTsdk": true,
  "explorer.file.reveal.auto": true,
  "cSpell.userWords": [
    "Feishu",
    "Infof",
    "MIAOHUI",
    "WECHAT",
    "WXWORK",
    "Weixin",
    "Wxid",
    "alicloud",
    "bson",
    "camelsnek",
    "datetime",
    "gonanoid",
    "grpc",
    "juzi",
    "juzibot",
    "mongodb",
    "nestjs",
    "pnpm",
    "protobuf",
    "qmgo",
    "unmarshalling",
    "upsert",
    "vals",
    "wechaty",
    "wrapperspb"
  ],
  "svelte.enable-ts-plugin": true,
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"]
    }
  }
}
